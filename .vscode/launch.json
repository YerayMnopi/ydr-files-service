    "version": "0.2.0",
    "configurations": [
        {
            "name": "images Runserver",
            "type": "python",
            "request": "launch",
            "justMyCode": false,
            "program": "${workspaceFolder}/manage.py",
            "args": ["runserver", "0.0.0.0:9117"],
            "django": true,
            "presentation": {
              "hidden": false,
              "group": "LEGALREPORTS",
              "order": 1
            }
          },
          {
            "name": "images Test",
            "type": "python",
            "request": "launch",
            "module": "pytest",
            "cwd": "${workspaceRoot}",
            "env": {
              "PYTHONPATH": "./src"
            },
            "envFile": "${workspaceRoot}/.env",
            "console": "integratedTerminal",
            "presentation": {
              "hidden": false,
              "group": "LEGALREPORTS",
              "order": 2
            }
          },

    ]
}